{
    "name": "laravel-zero/foundation",
    "description": "This is a mirror from illuminate/foundation.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.2"
    },
    "autoload": {
        "files": [
            "src/Illuminate/Foundation/helpers.php"
        ],
        "psr-4": {
            "Illuminate\\": "src/Illuminate/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
